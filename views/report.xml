<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_invoice_document_inherit" inherit_id="account.report_invoice_document">
            <xpath expr="//t[@t-call='web.external_layout']" position="replace">
                <t t-call="web.html_container">
                    <div class="article" t-att-data-oe-model="o and o._name"
                        t-att-data-oe-id="o and o.id"
                        t-att-data-oe-lang="o and o.env.context.get('lang')">
                        <div class="header">
                            <div class="row">
                                <div class="col-2"
                                    style="border: 0px solid blue; float: left; width: 20%; height: 100%;">
                                    <img t-if="o.company_id.logo"
                                        t-att-src="image_data_uri(o.company_id.logo)"
                                        style="height: 110px; display: block; margin: auto; border: 0px solid red;"
                                        alt="Logo" />
                                </div>
                                <div class="col-4"
                                    style="border: 0px solid green; float: left; width: 80%; height: 100%; font-size: 12px;"
                                    align="center">
                                    <p>
                                        <t style="font-size:14px" t-esc="o.company_id.name" />
                                        <br />
                                        NIT: <t t-esc="o.company_id.vat" />
                                        <br />
                                        <t
                                            t-esc="o.company_id.street" />
                                        <br />
                                        <t
                                            t-esc="o.company_id.city" />, <t
                                            t-esc="o.company_id.country_id.name" />
                                        <br /> TEL: <t
                                            t-esc="o.company_id.phone" />
                                        <br /> E-mail: <t
                                            t-esc="o.company_id.email" />
                                    </p>
                                </div>
                                <div class="col-4" align="left"
                                    style="border: 0px solid green; float: left; width: 80%; height: 100%; font-size: 12px;">
                                    <strong>Tipo de organizacion:</strong>
                                    <span t-field="o.company_id.type_organization_id" />
                                    <br />
                                    <strong>Regimen:</strong>
                                    <span t-field="o.company_id.type_regime_id" />
                                    <br />
                                    <strong>Tipo de responsabilidad:</strong>
                                    <span t-field="o.company_id.type_liability_id" />
                                    <br />
                                    <strong>Codigo CIIU:</strong>
                                    <!--<span t-field="o.company_id.type_liability_id"/>-->
                                    <br />
                                    <strong>Tarifa de ICA:</strong>
                                    <!--<span t-field="o.company_id.type_liability_id"/>-->
                                    <br />
                                </div>
                                <div t-if="o.ei_is_not_test and o.ei_qr_data" class="col-2"
                                    align="left">
                                    <!-- With OCA -->
                                    <!-- <img
                                    t-att-src="'/report/qr/?value=%s&amp;error_correction=%s' %
                                    (o.ei_qr_data, 1)" style="width:100;height:100"/>-->
                                    <!-- With Odoo-->
                                    <img
                                        t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('QR', o.ei_qr_data, 100, 100)" />
                                    <!-- With image -->
                                    <!-- <span t-field="o.ei_qr_image"
                                    t-options="{'widget':'image'}"/>-->
                                </div>
                            </div>
                        </div>
                        <t t-set="o" t-value="o.with_context(lang=lang)" />
                        <t t-set="forced_vat" t-value="o.fiscal_position_id.foreign_vat" />
                        <!-- So that it appears in the footer of the report instead of the company
                        VAT if it's set -->
                        <t t-set="address" t-if="not o.env.company.ei_enable">
                            <address t-field="o.partner_id"
                                t-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;address&quot;, &quot;name&quot;], &quot;no_marker&quot;: True}" />
                            <div t-if="o.partner_id.vat" class="mt16">
                                <t t-if="o.company_id.country_id.vat_label"
                                    t-esc="o.company_id.country_id.vat_label" id="inv_tax_id_label" />
                                    <t
                                    t-else="">Tax ID</t>: <span t-field="o.partner_id.vat" />
                            </div>
                        </t>
                        <h2 class="mt16">
                            <span
                                t-if="o.env.company.ei_enable and o.ei_is_not_test and o.move_type == 'out_invoice' and o.state == 'posted'">
                                Factura de venta electr√≥nica No:
                            </span>
                            <span
                                t-if="o.env.company.ei_enable and not o.ei_is_not_test and o.move_type == 'out_invoice' and o.state == 'posted'">
                                Factura de venta
                            </span>
                            <span
                                t-if="not o.env.company.ei_enable and o.move_type == 'out_invoice' and o.state == 'posted'">
                                Factura
                            </span>
                            <span t-if="o.move_type == 'out_invoice' and o.state == 'draft'">
                                Borrador</span>
                            <span t-if="o.move_type == 'out_invoice' and o.state == 'cancel'">
                                Cancelada</span>
                            <span t-if="o.move_type == 'out_refund'">Notas Creditos</span>
                            <span t-if="o.move_type == 'in_refund'">Vendor Credit Note</span>
                            <span t-if="o.move_type == 'in_invoice'">Factura de Compra</span>
                            <span t-if="o.name != '/'" t-field="o.name" />
                        </h2>
                        <div>
                            <table class="table table-sm table-bordered">
                                <tr style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                        Cliente:</td>
                                    <td colspan="3">
                                        <t t-esc="o.partner_id.name" />
                                    </td>
                                </tr>
                                <tr style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'civil_registration'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            R.C.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'id_card'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            T.I.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'id_document'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            C.C.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'national_citizen_id'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            C.C.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'residence_document'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            T.E.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'foreign_id_card'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            C.E.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'rut'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            NIT: </td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'passport'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            PAS:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'external_id'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            D.I.E.:</td>
                                    </t>
                                    <t
                                        t-if="o.partner_id.l10n_latam_identification_type_id.l10n_co_document_code == 'diplomatic_card'">
                                        <td
                                            style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                            Tax ID:</td>
                                    </t>
                                    <td>
                                        <span t-field="o.partner_id.vat" />
                                    </td>
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                        Telefono:
                                    </td>
                                    <td>
                                        <span t-field="o.partner_id.phone" />
                                        <span t-field="o.partner_id.mobile" />
                                    </td>
                                </tr>
                                <tr style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                        Direccion:
                                    </td>
                                    <td>
                                        <span t-field="o.partner_id.street" />
                                        <span t-field="o.partner_id.street2" />
                                    </td>
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                        Ciudad:
                                    </td>
                                    <td>
                                        <span t-field="o.partner_id.city" />/ <span
                                            t-field="o.partner_id.country_id" />
                                    </td>
                                </tr>
                                <tr style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">Direccion
                                        de entrega:</td>
                                    <td colspan="3">
                                        <t t-esc="o.partner_shipping_id.street" />
                                    </td>
                                </tr>
                                <tr style="font-size:12px;line-height: 30%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">
                                        Correo:</td>
                                    <td colspan="3">
                                        <t t-esc="o.partner_id.email" />
                                    </td>
                                </tr>
                                <tr style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">Fecha
                                        De Factura:</td>
                                    <td>
                                        <t t-esc="o.invoice_date" />
                                    </td>
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">Fecha
                                        De Vencimiento:</td>
                                    <td>
                                        <t t-esc="o.invoice_date_due" />
                                    </td>
                                </tr>
                                <tr style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">Orden
                                        de venta:</td>
                                    <td>
                                        <t t-esc="o.invoice_origin" />
                                    </td>
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">Orden
                                        de Compra:</td>
                                    <td>
                                        <t t-esc="o.ref" />
                                    </td>
                                </tr>
                                <tr t-if="o.reversed_entry_id"
                                    style="font-size:12px;line-height: 90%;margin-left:-20px">
                                    <td
                                        style="background-color: #d1d1d1;font-weight: bolder;color:black">Nota
                                        Credito:</td>
                                    <td>
                                        <t t-esc="o.reversed_entry_id.name" />
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <div class="page">
                            <t t-set="display_discount"
                                t-value="any(l.discount for l in o.invoice_line_ids)" />
                            <table class="table table-sm" style="font-size:11px;line-height: 100%;"
                                name="invoice_line_table">
                                <thead>
                                    <tr>
                                        <th class="text-center"
                                            style="background-color: #d1d1d1;color:black">
                                            <span>Items</span>
                                        </th>
                                        <t t-set="num_item" t-value="0" />
                                        <th name="th_description" class="text-left"
                                            style="background-color: #d1d1d1;color:black">
                                            <span>Referencia</span>
                                        </th>
                                        <th name="th_description" class="text-left"
                                            style="background-color: #d1d1d1;color:black">
                                            <span>Nombre del producto</span>
                                        </th>
                                        <th name="th_quantity" class="text-right"
                                            style="background-color: #d1d1d1;color:black">
                                            <span>Unidad de medida</span>
                                        </th>
                                        <th name="th_quantity" class="text-right"
                                            style="background-color: #d1d1d1;color:black">
                                            <span>Cantidad</span>
                                        </th>
                                        <th name="th_priceunit"
                                            style="background-color: #d1d1d1;color:black"
                                            t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                            <span>Precio Unitario</span>
                                        </th>
                                        <th name="th_price_unit"
                                            style="background-color: #d1d1d1;color:black"
                                            t-if="display_discount"
                                            t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                            <span>Disc.%</span>
                                        </th>
                                        <th name="th_taxes"
                                            style="background-color: #d1d1d1;color:black"
                                            t-attf-class="text-left {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                            <span>Impuesto</span>
                                        </th>
                                        <th name="th_subtotal" class="text-right"
                                            style="background-color: #d1d1d1;color:black">
                                            <span
                                                groups="account.group_show_line_subtotals_tax_excluded">
                                                Total</span>
                                            <span
                                                groups="account.group_show_line_subtotals_tax_included">
                                                Total</span>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody class="invoice_tbody">
                                    <t t-set="current_subtotal" t-value="0" />
                                    <t t-set="lines"
                                        t-value="o.invoice_line_ids.sorted(key=lambda l: (-l.sequence, l.date, l.move_name, -l.id), reverse=True)" />
                                    <t t-foreach="lines" t-as="line">
                                        <t t-set="current_subtotal"
                                            t-value="current_subtotal + line.price_subtotal"
                                            groups="account.group_show_line_subtotals_tax_excluded" />
                                        <t t-set="current_subtotal"
                                            t-value="current_subtotal + line.price_total"
                                            groups="account.group_show_line_subtotals_tax_included" />
                                        <tr
                                            t-att-class="'bg-200 font-weight-bold o_line_section' if line.display_type == 'line_section' else 'font-italic o_line_note' if line.display_type == 'line_note' else ''">
                                            <t t-if="not line.display_type"
                                                name="account_invoice_line_accountable">
                                                <t t-set="num_item" t-value="num_item + 1" />
                                                <td class="text-center">
                                                    <span t-esc="num_item" />
                                                </td>
                                                <td name="account_invoice_line_name">
                                                    <span t-field="line.product_id.default_code"
                                                        t-options="{'widget': 'text'}" />
                                                </td>
                                                <td name="account_invoice_line_name">
                                                    <span t-field="line.product_id.name"
                                                        t-options="{'widget': 'text'}" />
                                                </td>
                                                <td class="text-right">
                                                    <span t-field="line.product_uom_id"
                                                        groups="uom.group_uom" />
                                                </td>
                                                <td class="text-right">
                                                    <span t-field="line.quantity" />
                                                </td>
                                                <td
                                                    t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                                    <span class="text-nowrap"
                                                        t-field="line.price_unit" />
                                                </td>
                                                <td t-if="display_discount"
                                                    t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                                    <span class="text-nowrap"
                                                        t-field="line.discount" />
                                                </td>
                                                <td
                                                    t-attf-class="text-left {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                                    <t t-set="taxes"
                                                        t-value="line.tax_ids.filtered(lambda l: l.edi_tax_id and l.edi_tax_id.code == '01')" />
                                                    <span
                                                        t-esc="'\n '.join(map(lambda x: (x.description or x.edi_tax_id.name  ), taxes))"
                                                        id="line_tax_ids" />
                                                </td>
                                                <td class="text-right o_price_total">
                                                    <span class="text-nowrap"
                                                        t-field="line.price_subtotal"
                                                        groups="account.group_show_line_subtotals_tax_excluded" />
                                                    <span class="text-nowrap"
                                                        t-field="line.price_total"
                                                        groups="account.group_show_line_subtotals_tax_included" />
                                                </td>
                                            </t>
                                            <t t-if="line.display_type == 'line_section'">
                                                <td colspan="99">
                                                    <span t-field="line.name"
                                                        t-options="{'widget': 'text'}" />
                                                </td>
                                                <t t-set="current_section" t-value="line" />
                                                <t t-set="current_subtotal" t-value="0" />
                                            </t>
                                            <t t-if="line.display_type == 'line_note'">
                                                <td colspan="99">
                                                    <span t-field="line.name"
                                                        t-options="{'widget': 'text'}" />
                                                </td>
                                            </t>
                                        </tr>
                                        <t
                                            t-if="current_section and (line_last or lines[line_index+1].display_type == 'line_section')">
                                            <tr class="is-subtotal text-right">
                                                <td colspan="99">
                                                    <strong class="mr16">Subtotal</strong>
                                                    <span t-esc="current_subtotal"
                                                        t-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: o.currency_id}" />
                                                </td>
                                            </tr>
                                        </t>
                                    </t>
                                </tbody>
                            </table>
                            <div class="clearfix" style="font-size:12px;line-height: 100%;">
                                <div id="total" class="row">
                                    <div class="col-7">
                                        <div t-if="o.ei_is_valid">
                                            <!-- TO DO: CUDE cuando es nota debito                                   -->
                                            <strong t-if="o.move_type == 'out_invoice'"
                                                sytle="font-weight: bolder;color:black">CUFE:</strong>
                                            <strong t-if="o.move_type == 'out_refund'"
                                                sytle="font-weight: bolder;color:black">CUDE:</strong>
                                            <span style="font-size:9px;line-height: 100%;"
                                                t-field="o.ei_uuid" />
                                        </div>
                                        <div t-if="o.ei_signature">
                                            <strong sytle="font-weight: bolder;color:black">Firma:</strong>
                                            <span style="font-size:9px;line-height: 100%;"
                                                t-field="o.ei_signature" />
                                        </div>
                                        <div> Nota: <span t-field="o.company_id.footer_custom_text" />
                                        </div>
                                    </div>
                                    <div class="col-5">
                                        <div>
                                            <table class="table table-sm"
                                                style="page-break-inside: avoid;">
                                                <!--Tax totals-->
                                                <tr class="border-black o_total" style="">
                                                    <td>
                                                        <strong>Moneda</strong>
                                                    </td>
                                                    <td class="text-right">
                                                        <span t-field="o.currency_id" />
                                                    </td>
                                                </tr>
                                                <t t-set="tax_totals"
                                                    t-value="json.loads(o.tax_totals_json)" />
                                                <t t-call="account.document_tax_totals" />
                                                <!--Payments-->
                                                <tr class="border-black o_total" style=""
                                                    t-if="o.env.company.ei_enable"></tr>
                                                <t t-if="print_with_payments">
                                                    <t t-if="o.payment_state != 'invoicing_legacy'">
                                                        <t t-set="payments_vals"
                                                            t-value="o.sudo()._get_reconciled_info_JSON_values()" />
                                                        <t t-foreach="payments_vals"
                                                            t-as="payment_vals">
                                                            <tr>
                                                                <td>
                                                                    <i
                                                                        class="oe_form_field text-right oe_payment_label">
                                                                        Pagos <t
                                                                            t-esc="payment_vals['date']"
                                                                            t-options="{&quot;widget&quot;: &quot;date&quot;}" />
                                                                    </i>
                                                                </td>
                                                                <td class="text-right">
                                                                    <span
                                                                        t-esc="payment_vals['amount']"
                                                                        t-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: o.currency_id}" />
                                                                </td>
                                                            </tr>
                                                        </t>
                                                        <t t-if="len(payments_vals) &gt; 0">
                                                            <tr class="border-black">
                                                                <td>
                                                                    <strong>Importe Adeudado</strong>
                                                                </td>
                                                                <td class="text-right">
                                                                    <span
                                                                        t-field="o.amount_residual" />
                                                                </td>
                                                            </tr>
                                                        </t>
                                                    </t>
                                                </t>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <p
                                t-if="o.move_type in ('out_invoice', 'in_refund') and o.payment_reference"
                                name="payment_communication"> Utilice la siguiente referencia para
                                su pago: <b>
                                    <span t-field="o.payment_reference" />
                                </b>
                            </p>
                            <p t-if="o.invoice_payment_term_id" name="payment_term">
                                <span t-field="o.invoice_payment_term_id.note" />
                            </p>
                            <div t-if="not is_html_empty(o.narration)" name="comment">
                                <span t-field="o.narration" />
                            </div>
                            <p t-if="not is_html_empty(o.fiscal_position_id.note)" name="note">
                                <span t-field="o.fiscal_position_id.note" />
                            </p>
                            <p t-if="o.invoice_incoterm_id" name="incoterm">
                                <strong>Incoterm: </strong>
                                    <span
                                    t-field="o.invoice_incoterm_id.code" /> - <span
                                    t-field="o.invoice_incoterm_id.name" />
                            </p>
                            <div t-if="o.value_letters">
                                <strong>Valor en letras:</strong>
                                <span style="font-size:11px;line-height: 100%;"
                                    t-field="o.value_letters" />
                            </div>
                            <div t-if="o.env.company.ei_enable and o.ei_is_not_test"
                                class="container">
                                <div class="row">
                                    <div class="col-12" style="margin-left:-16px">
