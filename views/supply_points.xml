<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="supply_points_tree" model="ir.ui.view">
    <field name="name">Islas</field>
    <field name="model">supply.points</field>
    <field name="arch" type="xml">
      <tree>
        <field name="IdIsla"/>
        <field name="Nombre"/>
        <field name="IdEstacion"/>
      </tree>
    </field>
  </record>
  
  <record id="supply_points_action" model="ir.actions.act_window">
    <field name="name">Islas</field>
    <field name="res_model">supply.points</field>
    <field name="view_mode">tree,form</field>
  </record>
  
  <menuitem 
    id="action_supply_points" 
    action="supply_points_action"  
    name="Islas"
  />
</odoo>
